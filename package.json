{
  "scripts": {
    "test": "jest --verbose",
    "coverage": "jest --coverage",
    "build": "babel src --out-dir build --extensions \".ts,.js\" --source-maps inline --ignore */__tests__",
    "start": "NODE_ENV=production node build/server.js",
    "dev": "tsc && npm run build && node build/server.js",
    "watch": "nodemon --watch src/ --ext \"ts,js,json\" --exec \"tsc && npm run build && node build/server.js\"",
    "prisma:reset": "prisma db push --force-reset && prisma db seed",
    "prisma:generate": "prisma generate",
    "lint": "eslint ."
  },
  "prisma": {
    "seed": "ts-node src/prisma/seed.ts",
    "schema": "src/prisma/schema.prisma"
  },
  "name": "job-app-tracker",
  "version": "1.0.0",
  "dependencies": {
    "@prisma/client": "^3.14.0",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "joi": "^17.6.0",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.10.0",
    "mysql": "^2.18.1"
  },
  "keywords": [],
  "author": "Nekiro",
  "license": "ISC",
  "description": "nice app",
  "devDependencies": {
    "@babel/cli": "^7.17.6",
    "@babel/core": "^7.17.9",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-typescript": "^7.17.12",
    "@jest-mock/express": "^1.4.5",
    "@types/bcryptjs": "^2.4.2",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.5.1",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/morgan": "^1.9.3",
    "babel-jest": "^27.5.1",
    "eslint": "^8.13.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^26.1.4",
    "jest": "^27.5.1",
    "jest-mock-extended": "^2.0.6",
    "nodemon": "^2.0.15",
    "prettier": "^2.6.2",
    "prisma": "^3.13.0",
    "rimraf": "^3.0.2",
    "ts-node": "^10.8.1",
    "typescript": "^4.6.4"
  }
}
